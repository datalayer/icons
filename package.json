{
  "name": "@datalayer/icons-all",
  "version": "0.0.0",
  "private": true,
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "style"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/datalayer/icons.git"
  },
  "homepage": "https://icons.datalayer.tech",
  "scripts": {
    "build": "yarn run build-icons && yarn build-app",
    "build-app": "tsc && vite build",
    "build-icons": "yarn run build-optimized && yarn run build-react",
    "build-optimized": "yarn run build-data && yarn build-fun",
    "build-data": "rimraf ./optimized/data && svgo --config=svgo.yaml -f ./svg/data -o ./optimized/data --pretty --indent=2",
    "build-fun": "rimraf ./optimized/fun && svgo --config=svgo.yaml -f ./svg/fun -o ./optimized/fun --pretty --indent=2",
    "build-react": "node ./scripts/build.js react",
    "build-vue": "node ./scripts/build.js vue",
    "clean": "rimraf lib build dist tsconfig.tsbuildinfo",
    "dev": "vite",
    "landing:app": "tsc --watch",
    "preview": "vite preview",
    "start": "vite"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@datalayer/primer-addons": "0.0.16",
    "@svgr/core": "^5.5.0",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@vitejs/plugin-react": "2.0.0-beta.1",
    "@vue/compiler-dom": "^3.0.5",
    "autoprefixer": "^10.4.14",
    "camelcase": "^6.0.0",
    "cheerio": "^1.0.0-rc.12",
    "html-to-image": "1.9.0",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "svgo": "^1.3.2",
    "typescript": "5.0.3",
    "vite": "3.0.0-beta.5"
  }
}
