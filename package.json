{
  "name": "@datalayer/icons",
  "version": "0.0.10",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "style"
  ],
  "scripts": {
    "landing:studio": "tsc --watch",
    "build": "tsc && vite build",
    "clean": "rimraf lib build dist tsconfig.tsbuildinfo",
    "dev": "vite",
    "preview": "vite preview",
    "start": "vite",
    "build-icons": "npm run build-optimized && npm run build-react && npm run build-vue",
    "build-react": "node ./scripts/build.js react",
    "build-vue": "node ./scripts/build.js vue",
    "build-optimized": "rimraf ./optimized && svgo --config=svgo.yaml -f ./svg -o ./optimized --pretty --indent=2"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^5.3.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@primer/octicons-react": "18.2.0",
    "@primer/react": "35.22.0",
    "@svgr/core": "^5.5.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "2.0.0-beta.1",
    "@vue/compiler-dom": "^3.0.5",
    "camelcase": "^6.0.0",
    "html-to-image": "1.9.0",
    "prettier": "^2.8.7",
    "rimraf": "^3.0.2",
    "svgo": "^1.3.2",
    "typescript": "5.0.3",
    "vite": "3.0.0-beta.5"
  }
}
